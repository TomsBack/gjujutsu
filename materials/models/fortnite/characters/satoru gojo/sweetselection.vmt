"VertexlitGeneric"
{
	"$basetexture" "models\Fortnite\Characters\Satoru Gojo/SweetSelection_D"
	"$bumpmap" "models\Fortnite\Characters\Satoru Gojo/SweetSelection_N"
    "$basemapalphaphongmask" "1"
	"$ambientocclusion"	"1"
  
	"$phong" "1"
               "$phongexponent" "1"
               "$phongboost" "1"
    "$phongtint" "[ 1 1 1 ]"
	"$phongwarptexture" "models\Fortnite\Characters\Satoru Gojo/cell shaded lightwarp"
    "$lightwarptexture" "models\Fortnite\Characters\Satoru Gojo/cell shaded lightwarp"
               "$phongfresnelranges" "[2.2 3.7 4]"
	"$halflambert" "1"


	// Rim lighting parameters
	"$rimlight" "1"			
	"$rimlightexponent" "50"	
	"$rimlightboost" "0"

	"360?$color2" "[ 0.5 0.5 0.5 ]"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}