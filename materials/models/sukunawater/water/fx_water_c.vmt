"VertexLitGeneric"
{
  "$model" "1"
  "$basetexture" "models\thekins\water/water_hydro_base"
  "$bumpmap" "models\thekins\water/tfwater001_normal"
  "$envmap" "env_cubemap"
  "$envmaptint" "[.5 .5 .6]"


  "$refracttinttexture" "models\thekins\water/water_hydro_base"
  "$normalmap" "models\thekins\water/tfwater001_normal"
  "$dudvmap" "water/tfwater001_dudv"
  "$refractamount" ".100"
  "$bluramount" "1"


  "$envmapcontrast" ".5"
  $translucent 1
  "$ambientocclusion" "1"
  "$nocull" 1

  "$phong" "1"
  "$phongexponent" "20"
  "$phongboost" "1"  
  "$phongexponenttexture" "models\thekins\water/water_hydro_base"

  $phongfresnelranges   "[0 0.5 1]"
  "$halflambert" "0"

  // Rim lighting parameters
  "$rimlight" "1"                       // To enable rim lighting (requires phong)
  "$rimlightexponent" "1"                   // Exponent for phong component of rim lighting 
  "$rimlightboost" "0.5"                    // Boost for ambient cube component of rim lighting                   // Boost for ambient cube component of rim lighting

        "Proxies"
        {
            "TextureScroll"
            {
                "textureScrollVar" "$basetexturetransform"
                "textureScrollRate" 0.25
                "textureScrollAngle" 90
            }
        }
}
